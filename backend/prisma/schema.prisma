generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model UrlMapping {
  id        String   @id @default(cuid())
  shortId   String   @unique
  longUrl   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}